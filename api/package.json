{
  "name": "companies-profile",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_OPTIONS=--max-old-space-size=4096 nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:stag": "NODE_ENV=stagging nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "migration:run": "NODE_ENV=production yarn typeorm migration:run",
    "migration:revert": "NODE_ENV=production yarn typeorm migration:revert",
    "migration-dev:run": "NODE_ENV=development yarn typeorm migration:run",
    "migration-dev:revert": "NODE_ENV=development yarn typeorm migration:revert",
    "migration-dev:create": "NODE_ENV=development yarn typeorm-ts-node-commonjs migration:create",
    "typeorm": "typeorm-ts-node-commonjs -d src/config/typeorm-execute-migration.config.command.ts"
  },
  "dependencies": {
    "@fastify/basic-auth": "5.0.0",
    "@fastify/static": "^6.9.0",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "^9.3.10",
    "@nestjs/platform-fastify": "^9.3.10",
    "@nestjs/swagger": "6.2.1",
    "@nestjs/typeorm": "9.0.1",
    "@types/cache-manager": "4.0.1",
    "@types/config": "^3.3.0",
    "@types/nodemailer": "6.4.5",
    "@types/passport-jwt": "3.0.6",
    "cache-manager": "4.1.0",
    "cache-manager-redis-store": "2.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "config": "^3.3.9",
    "fastify": "4.5.3",
    "nestjs-typeorm-paginate": "4.0.2",
    "nodemailer": "6.7.8",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "pg": "8.7.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "typeorm": "0.3.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.0.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">= 14.19"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/migrations/",
      "<rootDir>/src/config/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "clearMocks": true,
    "moduleNameMapper": {
      "~/test/(.+)": "<rootDir>/test/$1",
      "~/(.+)": "<rootDir>/src/$1"
    }
  }
}
